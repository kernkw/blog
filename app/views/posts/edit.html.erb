<h1>TheCuriousRubyProgrammer</h1>
</br>
<h2>Editing Post: <%=@post.title%></h2>
  <%= form_for(@post) do |f| %>
    <p>
      <%= f.label :title %><br/>
      <%= f.text_field :title %>
    </p>
    <p>
      <%= f.label :author%><br/>
      <%= f.text_field :author %>
    </p>
    <p>
      <%= f.label :created_at%><br/>
      <%= f.date_field :created_at %>
    </p>
    <p>
      <%= f.label :body%><br/>
      <%= f.text_area :body, cols: 40, rows: 7 %>
    </p>
    <p>
      <%= f.submit %>
    </p>
  <%end%>
<nav>
  <%=link_to("Return to all", posts_path)%>
</nav>
